# API Keys for LLM Providers
# Get your API keys from the respective providers
# You only need to set the key for the provider you plan to use

# Override default model (optional)
LLMAN_DEFAULT_MODEL=gemini/gemini-2.5-flash-lite

# Gemini API Key
# Get your API key from: https://aistudio.google.com/apikey
GEMINI_API_KEY=your-gemini-api-key-here

# OpenAI API Key
# Get your API key from: https://platform.openai.com/api-keys
OPENAI_API_KEY=your-openai-api-key-here

# Anthropic API Key
# Get your API key from: https://console.anthropic.com/
ANTHROPIC_API_KEY=your-anthropic-api-key-here

# Groq API Key
# https://console.groq.com/
GROQ_API_KEY=your-groq-api-key-here

# Mistral API Key
# https://console.mistral.ai
MISTRAL_API_KEY=your-mistral-api-key-here

# Cohere API Key
# https://dashboard.cohere.com/api-keys
COHERE_API_KEY=your-cohere-api-key-here

# Together AI API Key
# https://api.together.xyz/settings/api-keys
TOGETHERAI_API_KEY=your-togetherapi-key-here

# Replicate API Key
# https://replicate.com/account/api-tokens
REPLICATE_API_KEY=your-replicate-api-key-here

# Hugging Face Inference Key
# https://huggingface.co/settings/tokens
HUGGINGFACE_API_KEY=your-huggingface-api-key-here

# xAI Grok API Key
XAI_API_KEY=your-xai-api-key-here

# OpenRouter API Key
OPENROUTER_API_KEY=your-openrouter-api-key-here

# DeepSeek API Key
DEEPSEEK_API_KEY=your-deepseek-api-key-here

# Perplexity API Key
PERPLEXITY_API_KEY=your-perplexity-api-key-here

# Vertex AI service account JSON
GOOGLE_APPLICATION_CREDENTIALS=/path/to/service-account.json

# Azure OpenAI
AZURE_OPENAI_API_KEY=your-azure-openai-api-key-here
# AZURE_OPENAI_ENDPOINT=https://your-resource-name.openai.azure.com/
# AZURE_OPENAI_DEPLOYMENT=gpt-4o

# AWS Credentials (for Bedrock / SageMaker)
AWS_ACCESS_KEY_ID=your-aws-access-key
AWS_SECRET_ACCESS_KEY=your-aws-secret-key
AWS_DEFAULT_REGION=us-east-1

# IBM watsonx
WATSONX_API_KEY=your-watsonx-api-key-here

# Databricks
DATABRICKS_TOKEN=your-databricks-token-here
